                                <div class="col-md-6">
                                    <input name="nome" placeholder="" class="form-control input-md" required="" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label" for="profissao">Profissão <h11>*</h11></label>
                                <div class="col-md-4">
                                    <input name="profissao" placeholder="" class="form-control" required="" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label" for="github">GitHub <h11>*</h11></label>
                                <div class="col-md-4">
                                    <input name="github" placeholder="" class="form-control" required="" type="text">
                                </div>
                            </div>
                            <div class="form-group">
