                            <div class="col-md-8 mb-4">
                                <button id="cadastrar" name="cadastrar" class="btn btn-success px-4 py-2" type="Submit">Cadastrar</button>
                                <button id="cancelar" name="cancelar" class="btn btn-danger px-4 py-2" type="Reset">Cancelar</button>
